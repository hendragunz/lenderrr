<%= form_for(resource, as: resource_name, url: registration_path(resource_name), class: 'dialog-form') do |f| %>
  <%= devise_error_messages! %>
    <%= f.text_field :username, html: {spellcheck: "false"}, class: 'span12', placeholder: 'Username' %>
    <br>
    <br>
    <%= f.email_field :email, html: {spellcheck: "false"}, class: 'span12', placeholder: 'Email' %>
    <br>
    <br>
    <%= f.password_field :password, html: {autocomplete: "off"}, class: 'span12', placeholder: 'Password' %>
    <br>
    <br>
    <%= f.password_field :password_confirmation, html: {autocomplete: "off"}, class: 'span12', placeholder: 'Password Confirmation' %>
     <br>
    <br>
    <%= f.submit "Register", class: 'btn btn-primary' %>

    <%#= render "devise/shared/links" %>

<% end %>
